    //		PerLimit     int64   `json:"perLimit"` // 每人限领张数
    //		MinPoint     float64 `json:"minPoint"` // 使用门槛；0表示无门槛
    //		StartTime    string  `json:"startTime"`
    //		EndTime      string  `json:"endTime"`
    //		UseType      int64   `json:"useType"`      // 使用类型：0->全场通用；1->指定分类；2->指定商品
