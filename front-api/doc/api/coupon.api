    //		EnableTime   string  `json:"enableTime"`   // 可以领取的日期
    //		Code         string  `json:"code"`         // 优惠码
    //		MemberLevel  int64   `json:"memberLevel"`  // 可领取的会员类型：0->无限时
    //	}
    //	UpdateCouponResp {
    //		Code    string `json:"code"`
